{
  "name": "smart-join-lines",
  "displayName": "smart-join-lines",
  "description": "join selected lines (even for non-space-separated natural language such as Japanese)",
  "publisher": "awtnb",
  "icon": "images/logo.png",
  "repository": {
    "url": "https://github.com/AWtnb/vscode-smart-join-lines"
  },
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Formatter",
    "Japanese",
    "Natural-language"
  ],
  "activationEvents": [
    "onCommand:smartJoinLines.join"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "smartJoinLines.join",
        "title": "smart-join-lines: join"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
